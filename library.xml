<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.sleepingdog.org.uk/heraldry"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.sleepingdog.org.uk/heraldry http://www.heraldicdevice.org.uk/xml/schema/heraldry/hedml-basic.xsd">
	<charges title="celestial">
		<charge type="star and crescent" subject="star, crescent">
			<svgUrls>
				<svgUrl href="star-and-crescent.svg"/>
			</svgUrls>
		</charge>
		<charge type="sun in splendour" subject="sun">
			<svgUrls>
				<svgUrl href="sun-in-splendour.svg"/>
			</svgUrls>
		</charge>
	</charges>
	<charges title="dragon">
		<charge type="dragon passant" subject="dragon" attitude="rampant">
			<svgUrls>
				<svgUrl href="dragon-passant.svg"/>
			</svgUrls>
		</charge>
	</charges>
	<charges title="eagle">
